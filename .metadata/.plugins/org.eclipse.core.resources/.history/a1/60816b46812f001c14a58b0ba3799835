package cloning_1.shallow;

public class MyClass implements Cloneable {

	String name = "Raymond";

	public MyClass(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public Object clone() throws CloneNotSupportedException {
		return super.clone();

	}

}
